{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "log:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://127.0.0.1:8000/*"
        },
        {
          "url": "http://localhost:8000/*"
        }
      ]
    }
  ]
}